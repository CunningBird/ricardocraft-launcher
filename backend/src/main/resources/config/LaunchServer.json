{
  "projectName": "ricardocraft",
  "mirrors": [
    "https://mirror.gravitlauncher.com/5.6.x/",
    "https://gravit-launcher-mirror.storage.googleapis.com/"
  ],
  "binaryName": "Launcher",
  "copyBinaries": true,
  "env": "STD",
  "auth": {
    "std": {
      "isDefault": true,
      "core": {
        "type": "memory"
      },
      "textureProvider": {
        "skinURL": "http://example.com/skins/%username%.png",
        "cloakURL": "http://example.com/cloaks/%username%.png",
        "type": "request"
      },
      "displayName": "Default",
      "visible": true
    }
  },
  "protectHandler": {
    "profileWhitelist": {},
    "allowUpdates": [],
    "type": "std"
  },
  "components": {
    "authLimiter": {
      "message": "Превышен лимит авторизаций",
      "exclude": [],
      "rateLimit": 3,
      "rateLimitMillis": 8000,
      "type": "authLimiter"
    },
    "proguard": {
      "modeAfter": "MainBuild",
      "dir": "proguard",
      "jvmArgs": [
        "-Xmx512M"
      ],
      "enabled": true,
      "mappings": true,
      "type": "proguard"
    }
  },
  "profileProvider": {
    "profilesDir": "profiles",
    "type": "local"
  },
  "updatesProvider": {
    "cacheFile": ".updates-cache",
    "updatesDir": "updates",
    "cacheUpdates": true,
    "type": "local"
  },
  "netty": {
    "fileServerEnabled": true,
    "ipForwarding": false,
    "disableWebApiInterface": false,
    "showHiddenFiles": false,
    "sendProfileUpdatesEvent": true,
    "launcherURL": "http://localhost:9274/Launcher.jar",
    "downloadURL": "http://localhost:9274/%dirname%/",
    "launcherEXEURL": "http://localhost:9274/Launcher.exe",
    "address": "ws://localhost:9274/api",
    "bindings": {},
    "performance": {
      "usingEpoll": false,
      "bossThread": 2,
      "workerThread": 8,
      "schedulerThread": 2,
      "maxWebSocketRequestBytes": 1048576,
      "disableThreadSafeClientObject": false,
      "executorType": "VIRTUAL_THREADS"
    },
    "security": {
      "hardwareTokenExpire": 28800,
      "publicKeyTokenExpire": 28800,
      "launcherTokenExpire": 28800
    },
    "binds": [
      {
        "address": "0.0.0.0",
        "port": 9274
      }
    ],
    "logLevel": "DEBUG"
  },
  "launcher": {
    "compress": true,
    "stripLineNumbers": true,
    "deleteTempFiles": true,
    "certificatePinning": false,
    "encryptRuntime": false,
    "customJvmOptions": [
      "-Dfile.encoding=UTF-8"
    ],
    "memoryLimit": 256
  },
  "sign": {
    "enabled": false,
    "keyStore": "pathToKey",
    "keyStoreType": "JKS",
    "keyStorePass": "mypass",
    "keyAlias": "myname",
    "keyPass": "mypass",
    "metaInfKeyName": "SIGNUMO.RSA",
    "metaInfSfName": "SIGNUMO.SF",
    "signAlgo": "SHA256WITHRSA",
    "checkCertificateExpired": true
  }
}